{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "plugins",
  "info": {
    "name": "Plugins"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "link": {
      "type": "string"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "tool": {
      "type": "enumeration",
      "enum": [
        "figma",
        "sketch",
        "adobexd"
      ]
    }
  }
}
